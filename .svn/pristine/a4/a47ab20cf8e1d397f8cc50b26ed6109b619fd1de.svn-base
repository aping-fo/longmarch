package com.game.sdk.proto.vo;

import com.google.common.collect.Lists;

import java.util.List;

/**
 *  家庭群信息
 */
public class FamilyGroupVO {
    private String id;
    private String creatorOpenId;
    private String name;
    private int score;
    private List<PlayerRankVO> groupMembers = Lists.newArrayListWithCapacity(8);

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getCreatorOpenId() {
        return creatorOpenId;
    }

    public void setCreatorOpenId(String creatorOpenId) {
        this.creatorOpenId = creatorOpenId;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getScore() {
        return score;
    }

    public void setScore(int score) {
        this.score = score;
    }

    public List<PlayerRankVO> getGroupMembers() {
        return groupMembers;
    }

    public void setGroupMembers(List<PlayerRankVO> groupMembers) {
        this.groupMembers = groupMembers;
    }
}
